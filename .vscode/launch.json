{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch Playground",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/source/Age.Playground/bin/Debug/net10.0/win-x64/Age.Playground.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": true,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false,
                "searchNuGetOrgSymbolServer": false,
            },
            "enableStepFiltering": true,
        },
        {
            "name": "(Windows) Launch StoryBook",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/source/Age.StoryBook/bin/Debug/net10.0/win-x64/Age.StoryBook.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": true,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false,
                "searchNuGetOrgSymbolServer": false,
            },
            "enableStepFiltering": true,
        },
    ]
}
