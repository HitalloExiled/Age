{

    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Playground",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/source/Age.Playground/bin/Debug/net10.0/Age.Playground",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": true,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false,
                "searchNuGetOrgSymbolServer": false,
            },
            "env": {
                "VK_INSTANCE_LAYERS": "VK_LAYER_KHRONOS_validation"
            },
            "enableStepFiltering": true,
        },
        {
            "name": "Launch StoryBook",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/source/Age.StoryBook/bin/Debug/net10.0/Age.StoryBook.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": true,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false,
                "searchNuGetOrgSymbolServer": false,
            },
            "enableStepFiltering": true,
        },
    ]
}
