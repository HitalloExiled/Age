{
    "dotnetCoreExplorer.logpanel": true,
    "dotnetCoreExplorer.searchpatterns": "**/tests/**/*.Tests.{dll,exe}",
    "dotnetCoreExplorer.codeLens": false,
    // "csharp.semanticHighlighting.enabled": false
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.spv": true,
        "**/TestResults": true,
        "**/bin": true,
        "**/obj": true,
        ".coverage": true,
        "**/*.bkp": true,
        "BenchmarkDotNet.Artifacts": true
    },
    "explorerExclude.backup": {},
    "glsllint.glslangValidatorArgs": "-V"
}
